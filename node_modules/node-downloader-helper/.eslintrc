{
    "parserOptions": {
        "sourceType": "module",
        "ecmaFeatures": {
            "globalReturn": true
        }
    },
    "env": {
        "node": true,
        "es6": true
    },
    "rules": {
        "quotes": [2,"single",{"avoidEscape": true}],
        "strict": [2,"never"]
    },
    "extends": [
        "eslint:recommended"
    ]
}